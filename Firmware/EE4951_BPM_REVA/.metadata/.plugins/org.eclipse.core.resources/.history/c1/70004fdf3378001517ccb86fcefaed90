void printADCvals(void)	{
	uint16_t vsense = ADC16_DRV_GetConvValueRAW(FSL_DUT_ADC,0);	//DUT
	uint16_t isense = ADC16_DRV_GetConvValueRAW(FSL_DUT_ADC,1);	//DUT
	static uint16_t count=0;
	uint8_t i =0;
	//for(i;i<2;i++)	{
	//	PRINTF("Channel %d:\t%d\n\r",i,ADC16_DRV_GetConvValueRAW(FSL_DUT_ADC,i));
	//}

	PRINTF("DUT Vsense = %.5d\tDUT Isense = %.5d\tCount: %d      \r",vsense, isense, count++);

}

