#ifndef SOURCES_SDCARD_H_
#define SOURCES_SDCARD_H_

#define MAXARRSIZE	256

class SDCard {
public:
	SDCard();
	virtual ~SDCard();

	uint8_t *getRxBuffer(void);
private:
	uint8_t txBuffer[MAXARRSIZE];
	uint8_t rxBuffer[MAXARRSIZE];
};

#endif /* SOURCES_SDCARD_H_ */
